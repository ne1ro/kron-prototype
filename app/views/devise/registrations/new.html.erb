<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => {id: 'sign_up', class: 'input_wrapper'}) do |f| %>
  <%= devise_error_messages! %>
  <%= f.text_field :nickname, placeholder: 'Login', :autofocus => true %>
    <%= f.password_field :password, placeholder: 'Password'%>
    <%= f.submit 'Sign Up', class: 'dotted' %>  
<% end %>

